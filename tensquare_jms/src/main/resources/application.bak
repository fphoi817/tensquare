
server:
  # ===================================================================
  # 端口号
  # ===================================================================
  port: 9009
  # ===================================================================
  # springboot内置Tomcat优化配置
  # ===================================================================
  tomcat:
    uri-encoding: UTF-8
    max-threads: 200
    min-spare-threads: 10
  # ===================================================================
  # 服务器链接超时
  # ===================================================================
  connection-timeout: 5000ms

spring:
  # ===================================================================
  # 项目名称
  # ===================================================================
  application:
    name: tensquare-jms

  # ===================================================================
  # springboot中配置springMvc异常抛出
  # ===================================================================
  mvc:
    throw-exception-if-no-handler-found: true
  # ===================================================================
  # JMS 消息中间件 RabbitMQ 配置
  # ===================================================================
  rabbitmq:
    host: 192.168.179.131